{
    "name": "ryangjchandler/filament-minimal-tabs",
    "description": "A clean and minimal design for the Tabs component in Filament.",
    "keywords": [
        "ryangjchandler",
        "laravel",
        "filament-minimal-tabs"
    ],
    "homepage": "https://github.com/ryangjchandler/filament-minimal-tabs",
    "support": {
        "issues": "https://github.com/ryangjchandler/filament-minimal-tabs/issues",
        "source": "https://github.com/ryangjchandler/filament-minimal-tabs"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0 | ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "RyanChandler\\FilamentMinimalTabs\\": "src/",
            "RyanChandler\\FilamentMinimalTabs\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RyanChandler\\FilamentMinimalTabs\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
